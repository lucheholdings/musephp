{
    "name": "musecalliope/calliope",
    "type": "library",
    "description": "The PHP SchemeModel Framework",
    "keywords": [
        "muse",
        "calliope"
    ],
    "license": "MIT",
    "homepage": "http://musephp.org",
    "authors": [
        {
            "name": "44",
            "email": "yoshi@44services.jp"
        }
    ],
    "support": {
        "email": "muse@ml.44services.jp",
        "source": "https://github.com/phpmuse/calliope"
    },
    "require": {
        "php": ">=5.3.2",
        "museclio/clio": "~1.0"
    },
    "require-dev": {
        "symfony/symfony": "~2.1.0",
        "jms/serializer":  "~0.13",
        "doctrine/orm":    "~2.3.0"
    },
    "replace": {
        "musecalliope/framework-core": "self.version",
        "musecalliope/location-extension": "self.version"
    },
    "autoload": {
        "psr-0": {
            "Calliope": "src/"
        }
    },
    "minimum-stability": "dev",
    "bin": ["bin/calliope", "bin/calliope.php"],
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
