<?xml version="1.0" encoding="UTF-8" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

	<parameters>
		<parameter key="terpsichore_oauth2_server.storage_strategy_factory.doctrine_cache.class">Terpsichore\Adapter\SymfonyBundles\OAuth2ServerBundle\DoctrineCache\Factory\ManagerFactory</parameter>
	</parameters>
	<services>
		<!-- DoctrineCache StorageFactory -->
        <service 
			id="terpsichore_oauth2_server.storage_strategy_factory.token.doctrine_cache" 
			class="%terpsichore_oauth2_server.storage_strategy_factory.doctrine_cache.class%" 
		>
			<argument type="service" id="service_container"/>
			<!-- DoctrineCache StrategyFactory only support token -->
			<tag name="terpsichore_oauth2_server.storage_strategy_factory.token" for="doctrine.cache"/>
        </service>
	</services>
</container>


