{
    "name": "museclio/clio",
    "type": "library",
    "description": "The PHP Libraries",
    "keywords": [
        "muse",
        "clio"
    ],
    "license": "MIT",
    "homepage": "http://musephp.org",
    "authors": [
        {
            "name": "44",
            "email": "yoshi@44services.jp"
        }
    ],
    "support": {
        "email": "muse@ml.44services.jp",
        "source": "https://github.com/phpmuse/clio"
    },
    "repositories": {
        "pear" : {
            "type": "pear",
            "url": "http://pear.php.net"
        }
    },
    "require": {
        "php": ">=5.3.2"
    },
    "replace": {
        "museclio/framework": "self.version",
        "museclio/util-container": "self.version",
        "museclio/util-type": "self.version",
        "museclio/util-metadata": "self.version"
    },
    "autoload": {
        "psr-0": {
            "Clio": "src/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}

